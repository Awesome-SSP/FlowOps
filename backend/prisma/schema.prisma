generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model contact {
  id               String    @id
  type             String?
  existingContacts String?
  firstName        String?
  lastName         String?
  suffix           String?
  title            String?
  status           String?   @default("Active")
  goesBy           String?
  pronouns         String?
  emailAddress     String?
  emails           Json?
  officeNumber     String?
  countryCode      String?
  cellNumber       String?
  addressLines     Json?
  addressLine2     String?
  city             String?
  state            String?
  zip              String?
  dateOfBirth      DateTime?
  workAnniversary  DateTime?
  maritalStatus    String?
  spouseName       String?
  childrensNames   Json?
  college          String?
  degree           String?
  priorEmployer    String?
  priorEmployers   Json?
  endDate          DateTime?
  notes            String?
  sportsTeam       String?
  favorites        String?
  group            String?
  report           String?
  createdAt        DateTime  @default(now())
}

model userregisteration {
  id               String   @id
  type             String?
  existingContacts String?
  firstName        String?
  lastName         String?
  email            String
  confirmEmail     String?
  phoneNo          String?
  role             String?
  userGroup        String   @default("None Selected")
  createdAt        DateTime @default(now())
}

model usersignup {
  id         String   @id
  firstName  String?
  lastName   String?
  email      String   @unique(map: "UserSignup_email_key")
  password   String
  ipAddress  String?
  macAddress String?
  createdAt  DateTime @default(now())
}
